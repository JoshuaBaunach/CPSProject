Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"This", " ", "function", " ", "randomizes", " ", "a", " ", 
    RowBox[{"list", ".", " ", 
     RowBox[{"I", "'"}]}], "m", " ", "using", " ", "this", " ", "instead", 
    " ", "of", " ", "RandomPermutation", " ", "because", " ", 
    "RandomPermutation", " ", "has", " ", "proven", " ", "to", " ", "be", " ",
     "unreliable", " ", "after", " ", "some", " ", 
    RowBox[{"testing", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"randomizeList", "[", "lst_", "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
        RowBox[{"newLst", "=", 
         RowBox[{"{", "}"}]}], ",", 
        RowBox[{"oldLst", "=", "lst"}], ",", "takeFrom"}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"takeFrom", "=", 
           RowBox[{"RandomInteger", "[", 
            RowBox[{"{", 
             RowBox[{"1", ",", 
              RowBox[{"Length", "[", "oldLst", "]"}]}], "}"}], "]"}]}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"AppendTo", "[", 
           RowBox[{"newLst", ",", 
            RowBox[{"oldLst", "[", 
             RowBox[{"[", "takeFrom", "]"}], "]"}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"oldLst", "=", 
           RowBox[{"Drop", "[", 
            RowBox[{"oldLst", ",", 
             RowBox[{"{", "takeFrom", "}"}]}], "]"}]}], ";"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", 
          RowBox[{"n", ",", "1", ",", 
           RowBox[{"Length", "[", "oldLst", "]"}]}], "}"}]}], 
        "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "newLst", "]"}], ";"}]}], "\[IndentingNewLine]",
      "]"}]}], ";"}]}]], "Input",
 CellChangeTimes->{{3.639068993665642*^9, 3.6390691416811075`*^9}, {
  3.639069226812977*^9, 3.639069305038451*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{
