       RowBox[{"categoryQLength", "=", 
        RowBox[{"{", "}"}]}], ",", 
       RowBox[{"catAppearance", "=", 
        RowBox[{"{", "}"}]}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"totalCategories", "=", 
       RowBox[{"Length", "[", "topic", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Generate", " ", "order", " ", "in", " ", "which", " ", "questions", 
        " ", "will", " ", "appear", " ", "per", " ", "category"}], " ", 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"Do", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"totalQuestions", "+=", 
          RowBox[{
           RowBox[{"Length", "[", 
            RowBox[{"topic", "[", 
